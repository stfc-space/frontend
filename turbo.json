{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "@radion/ui#build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "svelte.config.js",
        "vite.config.js",
        "src/**"
      ],
      "outputs": [
        "package/**"
      ],
      "outputMode": "new-only"
    },
    "itty-durable#build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "rollup.config.mjs",
        "src/**"
      ],
      "outputs": [
        "dist/**"
      ],
      "outputMode": "new-only"
    },
    "adapter-cloudfront#build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "src/**"
      ],
      "outputs": [
        "dist/**"
      ],
      "outputMode": "new-only"
    },
    "stfc-space-user-storage#build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "src/**"
      ],
      "outputMode": "new-only"
    },
    "stfc.space#build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "svelte.config.js",
        "vite.config.js",
        "src/**"
      ],
      "outputs": [
        ".cloudflare/**"
      ],
      "outputMode": "new-only"
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "src/**",
        "scripts/**",
        "shared/**",
        "templates/**"
      ],
      "outputs": [
        "files/**",
        "dist/**",
        ".svelte-kit/**"
      ],
      "outputMode": "new-only"
    },
    "check": {
      "dependsOn": [
        "prepare"
      ],
      "inputs": [
        "src/**",
        "types/**",
        ".svelte-kit/types/**",
        ".svelte-kit/tsconfig.json"
      ],
      "outputs": [],
      "outputMode": "new-only"
    },
    "format": {
      "cache": false,
      "outputs": [],
      "outputMode": "new-only"
    },
    "prepare": {
      "inputs": [
        "src/**",
        "source/**"
      ],
      "outputs": [
        ".svelte-kit/types/**",
        ".svelte-kit/tsconfig.json"
      ],
      "outputMode": "new-only"
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "coverage/",
        "test-results/**"
      ],
      "outputMode": "new-only",
      "env": [
        "CI",
        "TURBO_CACHE_KEY"
      ]
    },
    "lint": {
      "outputs": [],
      "outputMode": "new-only"
    },
    "precommit": {
      "dependsOn": [
        "format",
        "lint"
      ],
      "outputs": [],
      "outputMode": "new-only"
    },
    "dev": {
      "cache": false,
      "outputMode": "new-only"
    }
  },
  "globalDependencies": [
    ".eslintrc.json",
    ".prettierrc",
    "tsconfig.json"
  ]
}